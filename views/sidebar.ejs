<!-- product left -->
<div class="side-bar col-md-3">
  <div class="search-hotel">
    <form action="index" method="get">
    <h3 class="agileits-sear-head">Search Here..</h3>
    <div class="agileits-navi_search">
        <select id="agileinfo-nav_search" name="category" required="">
          <option >All Categories</option>
          <option value="books">Books</option>
          <option value="clothings">Clothings</option>
          <option value="electronics">Electronics</option>
          <option value="food &amp; Beverages">Food & Beverages</option>
          <option value="furniture">Furniture</option>
          <option value="kitchenware">Kitchenware</option>
          <option value="party Sets">Party Sets</option>
          <option value="tools">Tools</option>
          <option value="video Games">Video Games</option>
          <option value="vehicles">Vehicles</option>
          <option value="vouchers">Vouchers</option>
          <option value="others">Others</option>
        </select>
    </div>
      <input type="search" placeholder="Product name..." name="keywords" required="">
  </div>
  <!-- price range -->
  <div class="range">
    <h3 class="agileits-sear-head">Price range</h3>
    <ul class="dropdown-menu6">
      <li>
        <div id="slider-range"></div>
        <input type="text" id="amount" style="border: 0; color: #ffffff; font-weight: normal;" />
      </li>
    </ul>
  </div>
  <!-- //price range -->
  <!-- time range -->
  <div class="range">
    <h3 class="agileits-sear-head">Time range</h3>
    <ul class="dropdown-menu6">
      <%- include calender.ejs %>
    </ul>
  </div>
  <!-- //time range -->
  <!-- location preference -->
  <div class="left-side">
    <h3 class="agileits-sear-head">Location Preference</h3>
      <input type="radio" class="checked" name="location" value=""> Anywhere<br>
      <input type="radio" class="checked" name="location" value="central"> Central Singapore<br>
      <input type="radio" class="checked" name="location" value="north"> North Singapore<br>
      <input type="radio" class="checked" name="location" value="south"> South Singapore<br>
      <input type="radio" class="checked" name="location" value="east"> East Singapore<br>
      <input type="radio" class="checked" name="location" value="west"> West Singapore<br>
  </div>
  <!-- //location preference -->
  <!-- reviews -->
  <div class="customer-rev left-side">
    <h3 class="agileits-sear-head">Customer Review</h3>
          <input type="radio" class="checked" name="rating" value="5">
          <i class="fa fa-star" aria-hidden="true"></i>
          <i class="fa fa-star" aria-hidden="true"></i>
          <i class="fa fa-star" aria-hidden="true"></i>
          <i class="fa fa-star" aria-hidden="true"></i>
          <i class="fa fa-star" aria-hidden="true"></i>
          <span>5.0</span><br>
        <input type="radio" class="checked" name="rating" value="4">
          <i class="fa fa-star" aria-hidden="true"></i>
          <i class="fa fa-star" aria-hidden="true"></i>
          <i class="fa fa-star" aria-hidden="true"></i>
          <i class="fa fa-star" aria-hidden="true"></i>
          <i class="fa fa-star-o" aria-hidden="true"></i>
          <span>4.0+</span><br>
        <input type="radio" class="checked" name="rating" value="3">
          <i class="fa fa-star" aria-hidden="true"></i>
          <i class="fa fa-star" aria-hidden="true"></i>
          <i class="fa fa-star" aria-hidden="true"></i>
          <i class="fa fa-star-o" aria-hidden="true"></i>
          <i class="fa fa-star-o" aria-hidden="true"></i>
          <span>3.0+</span><br>
        <input type="radio" class="checked" name="rating" value="2">
          <i class="fa fa-star" aria-hidden="true"></i>
          <i class="fa fa-star" aria-hidden="true"></i>
          <i class="fa fa-star-o" aria-hidden="true"></i>
          <i class="fa fa-star-o" aria-hidden="true"></i>
          <i class="fa fa-star-o" aria-hidden="true"></i>
          <span>2.0+</span><br>
        <input type="radio" class="checked" name="rating" value="1">
          <i class="fa fa-star" aria-hidden="true"></i>
          <i class="fa fa-star-o" aria-hidden="true"></i>
          <i class="fa fa-star-o" aria-hidden="true"></i>
          <i class="fa fa-star-o" aria-hidden="true"></i>
          <i class="fa fa-star-o" aria-hidden="true"></i>
          <span>1.0+</span><br><br>
          <input type="submit" value="Search">
    </form>
  </div>
